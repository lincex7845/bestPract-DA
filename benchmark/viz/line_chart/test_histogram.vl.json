{
    "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
    "description": "xxx",
    "width": 500, "height": 400,
    "data": {"url": "data_histogram.csv", "format":{
        "type": "csv"
    }},
    "selection": {
      "grid": {
        "type": "interval", "bind": "scales"
      }
    },
    "mark": "line",
    "encoding": {
      "x": {
          "field":"id",
          "type": "quantitative"
      },
      "y": {
        "field":"ms",
        "type": "quantitative"
      },
      "color": {
        "field": "series",
        "type": "nominal",
        "scale": {"scheme": "dark2"}
      }
    }
  }